# Ignore directories managed by Composer.
/drush/contrib/
/vendor/
/web/core/
/web/libraries/
/web/modules/contrib/
/web/profiles/contrib/
/web/themes/contrib/

# Ignore sensitive information
/web/sites/sites.php
/web/sites/*/settings.php
